<SDAW>
  <filetype>STAKOPFSAETZE</filetype>
  <version>SDAW Version 004</version>
  <myversion>SDAW 1</myversion>
  <description>
    Kopfdaten der eingepflegten Dateien. Zur Versionierung STA, VMS, FRE.
  </description>
  <dataidentifier>K</dataidentifier>
  <!--    <mode>REPLACE</mode>-->
  <sdawfieldset>
    <!-- field template
		<field>
			<title></title>
			<type></type>
			<startpos></startpos>
			<length></length>
			<description></description>
			<valid></valid>
		</field>
	-->
    <!--
        Kopfsatz (STA Version 004)
            Feld Pos. Art Länge Gültige Werte
            Satzart 1 Char 001 K Kopfsatz
            Dateiart 2 Char 003 STA STA - Stammdaten
            GJ 5 Num 004 gültiges Geschäftsjahr
            Log.Dateikz 9 Num 019 SSSVVV9999999999999 Identifikator für log. Datei
            SSS = Senderidentifikation,
            VVV=Version
            999.. = Eindeutige Ident.nummer
            Datum 28 Num 008 DDMMJJJJ Dateierstellungsdatum
            Satzartkz 36 Char 001 D/S/X/T Satzarten innerhalb der logischen Datei
            ´D´ - Nur Einzelsätze
            ´S´ - Nur Summensätze
            ´X´ - Gemischte Datensätze
            ‚T’ – Teilbestand (Einzelsätze)
            xxx Satzlänge = 36
        -->
    <field>
      <title>Dateiart</title>
      <type>Char</type>
      <startpos>2</startpos>
      <length>3</length>
      <description>
        Art der eingelesenen Datei (STA, VM, FRE, ...)
      </description>
      <valid>STA, VMS, FRE</valid>
    </field>

    <field>
      <title>Jahr</title>
      <type>Char</type>
      <startpos>5</startpos>
      <length>4</length>
      <description>
        aktuelles Geschäftsjahr
      </description>
      <valid></valid>
    </field>

    <field>
      <title>LogDateiKennZiffer</title>
      <type>Char</type>
      <startpos>9</startpos>
      <length>19</length>
      <description>
        Log Datei-Kennung des Senders
      </description>
      <valid></valid>
    </field>

    <field>
      <title>Datum</title>
      <type>Char</type>
      <startpos>28</startpos>
      <length>8</length>
      <description>
        Erstellungsdatum der Datei DDDMMJJJJ
      </description>
      <valid></valid>
    </field>

    <field>
      <title>Satzarten</title>
      <type>Char</type>
      <startpos>36</startpos>
      <length>1</length>
      <description>
        enthaltene Satzarten
      </description>
      <valid>D,S,X,T</valid>
    </field>
  </sdawfieldset>
  <series>
    <field>
      <title>Jahr</title>
      <type>Char</type>
      <startpos>5</startpos>
      <length>4</length>
      <description>aktuelles Geschäfsjahr</description>
      <valid></valid>
    </field>
  </series>

  <keys>
    <index>Dateiart</index>
    <index>Jahr</index>
    <index>Datum</index>
    <primary>LogDateiKennZiffer</primary>
    <!-- <primary>be carefull</primary> -->
  </keys>
</SDAW>
